//Variables de Ciclo
unsigned long Cycle=30000;
unsigned long Duty = Cycle/3;

//FunciÃ³n de cambio de valores
void CambioValores(){
  if((millis()%Cycle>0) && (millis()%Cycle<Duty)){
    //Valores primeros 3s
  }
  else if((millis()%Cycle>Duty) && (millis()%Cycle<(2*Duty))){
    //Valores segundos 3s
  }
  else if((millis()%Cycle>(2*Duty)) && (millis()%Cycle<Cycle)){
    //Valores terceros 3s
  }
}
